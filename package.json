{
  "name": "sandeep-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "build:ssr": "ng build",
    "build:prerender": "ng build --prerender",
    "serve:ssr": "node dist/sandeep-portfolio/server/server.mjs",
