{
  "version": 2,
  "buildCommand": "ng build --configuration production --output-hashing=all",
  "outputDirectory": "dist/sandeep-portfolio/browser",
  "framework": null,
  "rewrites": [
    {
      "source": "/feedback/((?!general).*)",
      "destination": "/api/feedback/general"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|gif|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ]
}