{
  "version": 2,
  "name": "sandeep-portfolio",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/sandeep-portfolio"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/sandeep-portfolio",
  "installCommand": "npm install",
  "framework": "angular"
}